<div class="container">
{{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.image) }}
    {{ form_row(form.description) }}
    <ul>
        {% for videoField in form.video %}
            <li>
                {{ form_errors(videoField) }}
                {{ form_widget(videoField) }}
            </li>
        {% endfor %}
    </ul>
    {{ form_row(form.video) }}
    {{ form_rest(form) }}
<button class="btn btn-success mb-3">{{ button_label|default('Enregistrer') }}</button>

{{ form_end(form) }}
</div>
